              "$ref": "#/components/schemas/AddUser"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Registration is succesfull",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Successfully added! You can now connect to your account"
                    },
                    "Informations": {
                      "type": "object",
                      "properties": {
                        "datas": {
                          "type": "string",
                          "example": "object with user information formatted"
                        }
                      }
                    }
                  },
                  "xml": {
                    "name": "main"
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Successfully added! You can now connect to your account"
                    },
                    "Informations": {
                      "type": "object",
                      "properties": {
                        "datas": {
                          "type": "string",
                          "example": "object with user information formatted"
                        }
                      }
                    }
                  },
                  "xml": {
                    "name": "main"
                  }
                }
              }
            }
          },
          "403": {
            "description": "Registration is impossible",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message1": {
                      "type": "string",
                      "example": "Invalid or insufficient data for registration"
                    },
                    "message2": {
                      "type": "string",
                      "example": "User allready exist"
                    }
                  },
                  "xml": {
                    "name": "main"
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message1": {
                      "type": "string",
                      "example": "Invalid or insufficient data for registration"
                    },
                    "message2": {
                      "type": "string",
                      "example": "User allready exist"
                    }
                  },
                  "xml": {
                    "name": "main"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Server Error",
